<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="/socket.io/socket.io.js"></script>
    <script src="./library/popper.min.js"></script>
    <script src="./library/jquery-3.5.1.min.js"></script>
    <script src="./library/bootstrap.min.js"></script>
    <link rel="stylesheet" href="./library/bootstrap.min.css">
    <script type="module" src="./js/client.js"></script>
    <script type="module" src="./js/app_client.js"></script>
    <link rel="stylesheet" type="text/css" href="./css/app_client.css">
    
    <title>Funny Notebook</title>
</head>
<body>
  <div class="funnynotebook">
    <div class="top">
      <div role="banner">
        <div id="header" class="horizontal layout">
          <div id="header-background"></div>
          <div id="header-content">
            <div id="header-logo"></div>
            <div id="header-doc-toolbar" class="flex">
              <div id="document-title">
                <input id="doc-name" class="doc-name" aria-label="ノートブック名" maxlength="259" autocomplete="off" >
              </div>
              <button type="button" class="btn btn-light">ファイル</button>
              <button type="button" class="btn btn-light">編集</button>
              <button type="button" class="btn btn-light">表示</button>
              <button type="button" class="btn btn-light">挿入</button>
              <button type="button" class="btn btn-light">ランタイム</button>
            </div>
            <div id="header-right">           
              <button type="button" class="btn btn-light" >コメント</button>

            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="file-memu" id="file-memu" role="menu" aria-haspopup="tree">
    <div class="memu-item" >
      <div class="memu-item-content"></div>
    </div>
  </div>

  <div class="notebook">
    <div class="code cell">
      <div class="cell-toolbar">
        <button>UP</button>
        <button>DOWN</button>
      </div>
      <div class="main-code">
        <button class="code-mark">@</button>
        <form class="code-form">
          <input class="input-code" autocomplete="off">
        </form>
      </div>
      <div class="output">

      </div>
      <div class="add-cell">

      </div>
    </div>
    
  </div>

    <ul id="code_msg"></ul>
    <form id="code_form" action="#">
      <input id="input_code" autocomplete="off" /><button>Send</button>
    </form>
    <h1><%= title %></h1>
    <button type="button" class="btn btn-light">Light</button>
</body>
</html>